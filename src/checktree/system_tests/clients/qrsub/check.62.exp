proc check_option_a {} {
   global ts_config CHECK_OUTPUT
   
   set now [clock seconds]
   set then [expr $now + 10000]
   set start1 [clock format $then -format "%Y%m%d%H%M.%S"]
   set starto [clock format $then -format "%m/%d/%Y %H:%M:%S"]
   set then2 [expr $now + 20000]
   set start2 [clock format $then2 -format "%Y%m%d%H%M.%S"]
   
   # positive tests
   set args "-a $start1 -d 1000"
 
   set ar_id [submit_ar $args]
   if {$ar_id < 0} {
      return
   }

   puts $CHECK_OUTPUT "qrstat -ar $ar_id\n"
   set check_values(start_time)                     "$starto"
   set ret [parse_qrstat_check $ar_id check_values]
   if {$ret != 0 } {
      return
   }

   # negative tests
   set args "-a $start1"
 
   set ar_id [submit_ar $args "" "" 0]

   if {$ar_id != -55} {
      add_proc_error "submit_ar" -1 "error 'end time or duration' was not shown"
      return
   }

   set args "-a 0101010101 -d 1000"
 
   set ar_id [submit_ar $args "" "" 0]

   if {$ar_id != -58} {
      add_proc_error "submit_ar" -1 "error 'start time is in the past' was not shown"
      return
   }

   set args "-a $start2 -e $start1"
 
   set ar_id [submit_ar $args "" "" 0]

   if {$ar_id != -57} {
      add_proc_error "submit_ar" -1 "error 'start time is later than end time' was not shown"
      return
   }

   set args "-a errl -d 1000"
 
   set ar_id [submit_ar $args "" "" 0]

   if {$ar_id != -38} {
      add_proc_error "submit_ar" -1 "error $ar_id: 'Wrong date/time format' was not shown"
      return
   }
   puts $CHECK_OUTPUT "test check_option_a OK"
}

