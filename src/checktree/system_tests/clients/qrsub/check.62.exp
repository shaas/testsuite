# test specific globals:

# -------- local test procedures: initialization------------------------------

proc qrsub_setup {} {
   global ts_config CHECK_OUTPUT
   global actual_host 
   global qinstance 

   puts $CHECK_OUTPUT "qrsub test setup"

   set actual_host [host_conf_get_suited_hosts]

   set qinstance [get_queue_instance "all.q" $actual_host]

   set pe(pe_name)         "qrsub_pe"
   set pe(slots)           "10"
   set pe(allocation_rule) "\$round_robin"
   add_pe pe
   assign_queues_with_pe_object "all.q" "" "qrsub_pe"
}

proc qrsub_cleanup {} {
   global ts_config CHECK_OUTPUT

   delete_all_jobs
   delete_all_ars
   wait_for_end_of_all_jobs 60
   unassign_queues_with_pe_object "qrsub_pe"
   del_pe "qrsub_pe"
   puts $CHECK_OUTPUT "qrsub cleanup function"
}

proc qrsub_init_level {} {
   global CHECK_ACT_LEVEL

   if {$CHECK_ACT_LEVEL == 0} {
      return 0
   }

   return -1 
}

proc check_submit_ar {} {
   global ts_config CHECK_OUTPUT
 
   # positive tests
   set args "-d 1000"
 
   set ar_id [submit_ar $args]
   if {$ar_id < 0} {
      return
   }

   puts $CHECK_OUTPUT "test check_submit_ar OK"
}

proc check_submit_ar_noargs {} {
   global ts_config CHECK_OUTPUT
  
   # positive tests
   set args ""
 
   set ar_id [submit_ar $args "" "" 0]

   if {$ar_id != -36} {
      add_proc_error "check_submit_ar_noargs" -1 "error 'no options argument' was not shown"
      return
   }
   puts $CHECK_OUTPUT "test check_submit_ar_noargs OK"
}

proc check_submit_ar_invalid_arg {} {
   global ts_config CHECK_OUTPUT
  
   # negative tests
   set args "dfdf -d 1000"
 
   set ar_id [submit_ar $args "" "" 0]

   if {$ar_id != -32} {
      add_proc_error "check_submit_ar_noargs" -1 "error 'invalid option argument' was not shown"
