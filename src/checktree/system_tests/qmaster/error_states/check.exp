proc error_states_init_level {} {
   global CHECK_ACT_LEVEL
   global CHECK_PRODUCT_TYPE
   global test_array_jobs 

  switch -- $CHECK_ACT_LEVEL {
     "0" { 
           return 0    
     } 
     "1" {
           return -1
     }
  } 
  return -1  ;# no other level else
}

# -------- local test procedures -----------------------------------------------

global error_states_qconfig
global error_states_check_queue

proc error_states_setup  {} {
   global CHECK_OUTPUT
   global CHECK_CORE_EXECD
   global error_states_qconfig
   global error_states_check_queue

   set error_states_check_queue "[lindex $CHECK_CORE_EXECD 0 ].q"
   puts $CHECK_OUTPUT "saving configuration for queue $error_states_check_queue"
   get_queue $error_states_check_queue error_states_qconfig

   set_error 0 "ok"
}


proc error_states_prolog {} {
   global CHECK_OUTPUT CHECK_PRODUCT_ROOT CHECK_SCRIPT_FILE_DIR
   global error_states_check_queue CHECK_TESTSUITE_ROOT CHECK_ARCH

   set jobargs "-o /dev/null -e /dev/null $CHECK_PRODUCT_ROOT/examples/jobs/sleeper.sh" 


   set my_conf(prolog) "/tmp/no_such_file.23"
   set_queue $error_states_check_queue my_conf 

   set job_id [submit_job "-q $error_states_check_queue $jobargs 5" ]
   trigger_scheduling
   wait_for_jobpending $job_id "leeper" 60 1
   set q_state [wait_for_queue_state $error_states_check_queue "E" 60]
   
   set my_conf(prolog) "$CHECK_TESTSUITE_ROOT/$CHECK_SCRIPT_FILE_DIR/exit_01.sh"
   set_queue $error_states_check_queue my_conf 

   puts $CHECK_OUTPUT "clearing error state $q_state of queue $error_states_check_queue"
   catch {  eval exec "$CHECK_PRODUCT_ROOT/bin/$CHECK_ARCH/qmod" "-c $error_states_check_queue" } catch_result
   puts $CHECK_OUTPUT $catch_result

   trigger_scheduling
   wait_for_jobpending $job_id "leeper" 60 1
   set q_state [wait_for_queue_state $error_states_check_queue "E" 60]

   set my_conf(prolog) "$CHECK_TESTSUITE_ROOT/$CHECK_SCRIPT_FILE_DIR/exit_99.sh"
   set_queue $error_states_check_queue my_conf 

   puts $CHECK_OUTPUT "clearing error state $q_state of queue $error_states_check_queue"
   catch {  eval exec "$CHECK_PRODUCT_ROOT/bin/$CHECK_ARCH/qmod" "-c $error_states_check_queue" } catch_result
   puts $CHECK_OUTPUT $catch_result

   trigger_scheduling
   wait_for_jobpending $job_id "leeper" 60 1

   wait_for_job_state $job_id "R" 60

   set my_conf(prolog) "$CHECK_TESTSUITE_ROOT/$CHECK_SCRIPT_FILE_DIR/exit_00.sh"
   set_queue $error_states_check_queue my_conf 

   trigger_scheduling
   wait_for_jobpending $job_id "leeper" 60 1
   wait_for_jobstart $job_id "leeper" 60
   wait_for_jobend $job_id "leeper" 60

   set my_conf(prolog) "NONE"
   set_queue $error_states_check_queue my_conf 

   set_error 0 "ok"
}

proc error_states_epilog {} {
   global CHECK_OUTPUT CHECK_PRODUCT_ROOT CHECK_SCRIPT_FILE_DIR
   global error_states_check_queue CHECK_TESTSUITE_ROOT 

   set jobargs "-o /dev/null -e /dev/null $CHECK_PRODUCT_ROOT/examples/jobs/sleeper.sh" 


   set my_conf(epilog) "/tmp/no_such_file.23"
   set_queue $error_states_check_queue my_conf 


   set job_id [submit_job "-q $error_states_check_queue $jobargs 5" ]
   wait_for_jobpending $job_id "leeper" 60 1
   wait_for_jobstart $job_id "leeper" 60
   wait_for_jobend $job_id "leeper" 60
   # JG: TODO: this get_qacct will fail in 6.0 due to delayed writing of accounting information
   # set reporting_params, accounting_flush_time to 00:00:00, or poll one minute until
   # accounting info is available. Consider writing a function get_qacct_poll in sge_procedures
   # There are multiple calls to qacct in this check!
   get_qacct $job_id
   puts $CHECK_OUTPUT "qacct output for job $job_id:"
   puts $CHECK_OUTPUT "failed: $qacct_info(failed)"
   puts $CHECK_OUTPUT "exit_status:  $qacct_info(exit_status)"
   set qacct_error [lindex $qacct_info(failed) 0]
   set exit_state  [lindex $qacct_info(exit_status) 0]
   if { $exit_state != 0 } {
      add_proc_error "error_states_epilog" -1 "exit state not 0: $exit_state"
   }
   if { $qacct_error != 14 } {
      add_proc_error "error_states_epilog" -1 "failed not 14 (before epilog): $qacct_error"
   }

   set my_conf(epilog) "$CHECK_TESTSUITE_ROOT/$CHECK_SCRIPT_FILE_DIR/exit_01.sh"
   set_queue $error_states_check_queue my_conf 

   set job_id [submit_job "-q $error_states_check_queue $jobargs 15" ]
   wait_for_jobstart $job_id "leeper" 60
   wait_for_jobend $job_id "leeper" 60
   get_qacct $job_id
   puts $CHECK_OUTPUT "qacct output for job $job_id:"
   puts $CHECK_OUTPUT "failed: $qacct_info(failed)"
   puts $CHECK_OUTPUT "exit_status:  $qacct_info(exit_status)"

   set exit_state  [lindex $qacct_info(exit_status) 0]
   if { $exit_state != 0 } {
      add_proc_error "error_states_epilog" -1 "exit state not 0: $exit_state"
   }
  
   if { [ string first "15" $qacct_info(failed)] < 0 || 
        [ string first "in epilog" $qacct_info(failed)] < 0   } {
        add_proc_error "error_states_epilog" -1 "no in epilog error: $qacct_info(failed)"
   }

   set my_conf(epilog) "NONE"
   set_queue $error_states_check_queue my_conf 


   set_error 0 "ok"
}


proc error_states_no_job_shell {} {
   global CHECK_OUTPUT error_states_check_queue
   global CHECK_PRODUCT_ROOT

   set jobargs "-o /dev/null -e /dev/null $CHECK_PRODUCT_ROOT/examples/jobs/sleeper.sh" 
      
   set job_id [submit_job "-S /tmp/no_such_file.23 -q $error_states_check_queue $jobargs 10" ]
   wait_for_jobpending $job_id "leeper" 60 1
   wait_for_job_state $job_id "E" 60

   delete_job $job_id

   set_error 0 "ok"
}

proc error_states_no_default_queue_shell {} {
   global CHECK_OUTPUT error_states_check_queue
   global CHECK_PRODUCT_ROOT CHECK_ARCH CHECK_TESTSUITE_ROOT CHECK_SCRIPT_FILE_DIR

   set jobargs "-o /dev/null -e /dev/null $CHECK_TESTSUITE_ROOT/$CHECK_SCRIPT_FILE_DIR/exit_00.sh" 

   
   get_queue $error_states_check_queue org_conf

   puts $CHECK_OUTPUT "queue shell_start_mode is $org_conf(shell_start_mode)"

   set my_conf(shell) "/tmp/no_such_file.23"
   set my_conf(shell_start_mode) "posix_compliant"  
   set_queue $error_states_check_queue my_conf

   
   set job_id [submit_job "-q $error_states_check_queue $jobargs" ]
   wait_for_jobpending $job_id "exit" 60 1
   set q_state [wait_for_queue_state $error_states_check_queue "E" 60]
   
   set my_conf(shell) $org_conf(shell) 
   set_queue $error_states_check_queue my_conf 

   puts $CHECK_OUTPUT "clearing error state $q_state of queue $error_states_check_queue"
   catch {  eval exec "$CHECK_PRODUCT_ROOT/bin/$CHECK_ARCH/qmod" "-c $error_states_check_queue" } catch_result
   puts $CHECK_OUTPUT $catch_result


   wait_for_jobstart $job_id "exit" 60
   wait_for_jobend $job_id "exit" 60


   set my_conf(shell_start_mode) "unix_behavior"
   set_queue $error_states_check_queue my_conf
   set job_id [submit_job "-q $error_states_check_queue $jobargs" ]
   wait_for_jobpending $job_id "exit" 60 1
   wait_for_jobstart $job_id "exit" 60
   wait_for_jobend $job_id "exit" 60 
   set my_conf(shell_start_mode) $org_conf(shell_start_mode)
   set_queue $error_states_check_queue my_conf


   set_error 0 "ok"
}

