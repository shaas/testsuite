             continue
          }
          -i $sp_id "Please enter the name of the database user * >> " {
             send_to_spawn_id $sp_id [get_arco_read_user]
             continue
          }
          -i $sp_id "Please enter the password of the database user >> " {
             send_to_spawn_id $sp_id "secret"
             continue
          }
