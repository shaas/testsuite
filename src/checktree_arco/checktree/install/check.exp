          -i $sp_id "Remove directory * (y/n) * >> " {
             send_to_spawn_id $sp_id "y"
             continue
          }
          -i $sp_id "Should the ARCo reporting module * be unregistered? (y/n) * >> " {
             send_to_spawn_id $sp_id "y"
             continue
